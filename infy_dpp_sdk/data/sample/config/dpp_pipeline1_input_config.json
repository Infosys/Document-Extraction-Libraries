{
    "name": "pipeline1",
    "description": "Sample 4-processor-pipeline configuration for unit testing",
    "variables": {
        "DPP_SYS_PIPELINE_REQ_FILE_PATH": "<value will be auto updated>",
        "DPP_SYS_PIPELINE_INPUT_CONFIG_PATH": "<value will be auto updated>",
        "CHUNKED_FILES_ROOT_PATH": "/docwbvectordb/chunked",
        "ENCODED_FILES_ROOT_PATH": "/docwbvectordb/encoded",
        "WORK_ROOT_PATH": "/data/work/",
        "DPP_STORAGE_URI": "file://C:/workarea/documentworkbench",
        "TIKTOKEN_CACHE_DIR": "C:/workarea/documentworkbench/data/config/tiktoken_encoding"
    },
    "processor_list": [
        {
            "enabled": true,
            "processor_name": "document_downloader",
            "processor_namespace": "tests.use_cases.uc_01.processors.document_downloader",
            "processor_class_name": "DocumentDownloaderV1",
            "processor_input_config_name_list": [
                "DocumentDownloader"
            ]
        },
        {
            "enabled": true,
            "processor_name": "content_extractor",
            "processor_namespace": "tests.use_cases.uc_01.processors.content_extractor",
            "processor_class_name": "ContentExtractorV1",
            "processor_input_config_name_list": [
                "ContentExtractor"
            ]
        },
        {
            "enabled": true,
            "processor_name": "attribute_extractor",
            "processor_namespace": "tests.use_cases.uc_01.processors.attribute_extractor",
            "processor_class_name": "AttributeExtractorV1",
            "processor_input_config_name_list": [
                "AttributeExtractor"
            ]
        },
        {
            "enabled": true,
            "processor_name": "document_uploader",
            "processor_namespace": "tests.use_cases.uc_01.processors.document_uploader",
            "processor_class_name": "DocumentUploaderV1",
            "processor_input_config_name_list": [
                "DocumentUploader"
            ]
        }
    ],
    "processor_input_config": {
        "DocumentDownloader": {
            "readPath": "/data/input",
            "filter": {
                "include": [
                    "*.txt",
                    "*.jpg"
                ],
                "exclude": []
            },
            "writePath": "/data/work"
        },
        "ContentExtractor": {},
        "AttributeExtractor": {
            "required_tokens": [
                {
                    "name": "Company Name",
                    "position": 5
                },
                {
                    "name": "Country",
                    "position": 9
                }
            ]
        },
        "DocumentUploader": {
            "writePath": "/data/output"
        }
    }
}