    "processor_input_config": {
        "RequestCreator": {
            "read_path": "/input/",
            "batch_size": 20,
            "filter": {
                "include": [
                    "jpg",
                    "json"
                ],
                "exclude": [
                    "_"
                ]
            },
            "work_root_path": "/work/",
            "queue": {
                "enabled": true,
                "queue_root_path": "/work/queue/"
            }
        },
        "RequestCloser": {
            "queue": {
                "enabled": true,
                "queue_root_path": "/work/queue/"
            },
            "work_root_path": "/work/",
            "output_root_path": "/output/"
        },
        "DocumentDataSaver": {
            "work_root_path": "/work/"
        },
        "DocumentDataUpdater": {
            "config_data": [
                {
                    "enabled": true,
                    "json_path": "$.context_data.reader.output[*].model_output.sources[*]",
                    "json_key": "bbox",
                    "replace_key_enabled": false,
                    "replace_with_key": "",
                    "replace_with_value": "",
                    "replace_value_enabled": false,
                    "replace_value_with_rule_name": "rule_cus_001_transform_bbox_value"
                }
            ]
        },
        "SegmentGenerator": {
            "textProviders": [
                {
                    "provider_name": "tesseract_ocr_provider",
                    "properties": {
                        "tesseract_path": ""
                    }
                },
                {
                    "provider_name": "azure_read_ocr_provider",
                    "properties": {
                        "subscription_key": "",
                        "url": ""
                    }
                },
                {
                    "provider_name": "pdf_box_text_provider",
                    "properties": {}
                },
                {
                    "provider_name": "json_provider",
                    "properties": {
                        "template1_file_path": "/data/config/templates/email_template.txt"
                    }
                }
            ],
            "modelProviders": [
                {
                    "provider_name": "detectron",
                    "properties": {
                        "model_path": "",
                        "config_file_path": "",
                        "model_threshold": 0.8
                    }
                }
            ],
            "techniques": [
                {
                    "enabled": false,
                    "name": "technique1",
                    "input_file_type": "image",
                    "text_provider_name": "tesseract_ocr_provider",
                    "model_provider_name": "detectron"
                },
                {
                    "enabled": false,
                    "name": "technique2",
                    "input_file_type": "image",
                    "text_provider_name": "azure_read_ocr_provider",
                    "model_provider_name": "detectron"
                },
                {
                    "enabled": false,
                    "name": "technique3",
                    "input_file_type": "pdf",
                    "text_provider_name": "pdf_box_text_provider",
                    "model_provider_name": null
                },
                {
                    "enabled": false,
                    "name": "technique4",
                    "input_file_type": "pdf",
                    "text_provider_name": "pdf_box_text_provider",
                    "model_provider_name": "detectron"
                },
                {
                    "enabled": false,
                    "name": "technique5",
                    "input_file_type": "json",
                    "text_provider_name": "json_provider",
                    "model_provider_name": null
                },
                {
                    "enabled": true,
                    "name": "technique6",
                    "input_file_type": "image",
                    "text_provider_name": "azure_read_ocr_provider",
                    "model_provider_name": null
                }
            ]
        },
        "SegmentDataParser": {
            "layout": {
                "single-column": {
                    "enabled": true
                },
                "multi-column": {
                    "enabled": false
                }
            },
            "pattern": {
                "sequence-order": {
                    "enabled": true
                },
                "left-right": {
                    "enabled": false
                },
                "zig-zag": {
                    "enabled": false
                }
            }
        },
        "ChunkDataParser": {
            "chunking_method": "page",
            "merge_title_paragraph": false,
            "page_num": [
                "1:10"
            ],
            "exclude": [
                "table",
                "figure"
            ]
        },
        "SaveChunkDataParser": {
            "chunked_files_root_path": "/vectordb/chunked"
        }
    }
}