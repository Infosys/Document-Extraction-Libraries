{
    "name": "dask_pipeline",
    "description": "Sample pipeline for hybrid orchestrator unit testing",
    "variables": {},
    "processor_list": [
        {
            "enabled": true,
            "processor_name": "document_downloader",
            "processor_namespace": "infy_dpp_sdk.use_cases.uc_01.processors.document_downloader",
            "processor_class_name": "DocumentDownloaderV1",
            "processor_input_config_name_list": [
                "DocumentDownloader"
            ]
        },
        {
            "enabled": true,
            "processor_name": "content_extractor",
            "processor_namespace": "infy_dpp_sdk.use_cases.uc_01.processors.content_extractor",
            "processor_class_name": "ContentExtractorV1",
            "processor_input_config_name_list": [
                "ContentExtractor"
            ]
        },
        {
            "enabled": true,
            "processor_name": "attribute_extractor",
            "processor_namespace": "infy_dpp_sdk.use_cases.uc_01.processors.attribute_extractor",
            "processor_class_name": "AttributeExtractorV1",
            "processor_input_config_name_list": [
                "AttributeExtractor"
            ]
        },
        {
            "enabled": true,
            "processor_name": "document_uploader",
            "processor_namespace": "infy_dpp_sdk.use_cases.uc_01.processors.document_uploader",
            "processor_class_name": "DocumentUploaderV1",
            "processor_input_config_name_list": [
                "DocumentUploader"
            ]
        }
    ],
    "processor_input_config": {
        "DocumentDownloader": {
            "readPath": "/data/input",
            "filter": {
                "include": [
                    "*.txt",
                    "*.jpg"
                ],
                "exclude": []
            },
            "writePath": "/data/work"
        },
        "ContentExtractor": {},
        "AttributeExtractor": {
            "required_tokens": [
                {
                    "name": "Company Name",
                    "position": 5
                },
                {
                    "name": "Country",
                    "position": 9
                }
            ]
        },
        "DocumentUploader": {
            "writePath": "/data/output"
        }
    },
    "orchestrator": {
        "processor_invocation": {
            "default_mode": "cli",
            "dask": {
                "enabled": false,
                "scheduler_endpoint": ""
            },
            "custom_mode": {
                "native": {
                    "processor_names": [
                        "content_extractor"
                    ]
                },
                "cli": {
                    "processor_names": []
                },
                "http": {
                    "processor_names": [
                        "attribute_extractor"
                    ]
                }
            }
        }
    }
}